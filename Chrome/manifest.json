{
    "name": "TimeXtension",
    "description": "Manage your time online!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "keeptrack.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/16.png",
            "24": "/images/24.png",
            "32": "/images/32.png",
            "64": "/images/64.png",
            "128": "/images/128.png",
            "256": "/images/256.png",
            "512": "/images/512.png"
        }
    },
    "icons": {
        "16": "/images/16.png",
        "24": "/images/24.png",
        "32": "/images/32.png",
        "64": "/images/64.png",
        "128": "/images/128.png",
        "256": "/images/256.png",
        "512": "/images/512.png"
    },
    "permissions": ["tabs", "storage", "alarms"]
}